@model IEnumerable<WebApplication1.Models.Newss>
@{
    ViewBag.Title = "News_page";
}
<html>

<head>
    <title>Новости</title>
    @Styles.Render("~/Content/css")
</head>
<body bgcolor="#f2f2f2">

    <nav class="navbar navbar-expand-lg navbar-light">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
            <div class="navbar-nav">
                @Html.ActionLink("Главная", "Index", "Home", new { @class = "nav-link" })
                @Html.ActionLink("Новости", "News", "Home", new { @class = "nav-link_active" })
                @Html.ActionLink("Конференции", "Conferences", "Home", new { @class = "nav-link" })
                <a class="nav-link">Контакты</a>

                <a class="right">
                    <svg xmlns="http://www.w3.org/2000/svg" width="100%" height="90%" fill="currentColor" viewBox="0 -1 16 16">
                        <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001c.03.04.062.078.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1.007 1.007 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0z" />
                    </svg>
                </a>

                <a class="right">
                    <svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%" fill="currentColor" viewBox="0 0 16 16">
                        <path d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                    </svg>
                </a>
                <div class="way">Главная&emsp;&ensp;/&ensp;&emsp;Новости&emsp;&ensp;/&ensp;&emsp;@foreach(var b in Model){ @b.title }</div>
            </div>

        </div>
    </nav>

    @foreach (var b in Model)
    {
        <div class="news_content_main">
            <div class="content">

                <h1>@b.title</h1>
                <h2>@b.Subtitle</h2>
                <p><img class=" img_left" src="~/Assets/@b.Img" /> @b.FullText</p>
            </div>
        </div>
    }
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
    </body>

</html>
